    "bs = [0.01, 0.1, 0.2, 1]\n",
    "for b in bs:\n",
    "    test = IsingGibbs(b, 5, 50000,1000)\n",
    "    plt.hist(test[0], label='mean(M) = {:.4f}'.\\\n",
    "             format(np.mean(test[0])))\n",

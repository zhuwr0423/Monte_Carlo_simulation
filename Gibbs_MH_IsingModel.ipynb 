    "Ls = np.arange(10, 30, 1)\n",
    "iat_L = []\n",
    "for L in Ls:\n",
    "    mag = IsingGibbs(.01, L, 10000,1000)[0]\n",
    "    iat_L.append(autocorr(mag)[0])"
   ]

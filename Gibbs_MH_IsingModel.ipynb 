   "source": [
    "bs = [0.01, 0.1, 0.2 ]\n",
    "for b in bs:\n",
    "    mag_random = IsingGibbs(b, 5, 50000,1000)[0]\n",
    "    iatrandom, autorandom = autocorr(mag_random)\n",
    "    mag_det = IsingGibbsDet(b, 5, 50000,1000)[0]\n",
    "    iatdet, autodet = autocorr(mag_det)\n",
    "    plt.plot(np.arange(len(autorandom)),autorandom, label = 'random : IAT = {:.2f}'.\\\n",
    "          format(iatrandom))\n",
    "    plt.plot(np.arange(len(autodet)),autodet, label = 'deternimistic : IAT = {:.2f}'.\\\n",
    "         format(iatdet))\n",
    "    plt.legend(loc='best',fontsize=10)\n",
    "    plt.title('beta = {}'.format(b))\n",
    "    plt.show()"
   ]
  },

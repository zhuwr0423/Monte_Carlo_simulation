    "tems = np.arange(0.01, 1., .02)\n",
    "iat = []\n",
    "for tem in tems:\n",
    "    mag = IsingGibbs(tem, 5, 20000,1000)[0]\n",
    "    iat.append(autocorr(mag)[0])"
